                            android:id="@+id/cm0109m"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/cm0109m" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/cm0109mx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/cm0109m"
                            android:inputType="number"
                            android:maxLength="2"
                            android:tag="cm0109m"
                            android:text='@{cm0109m.checked ? cm0109mx.getText().toString() : ""}'
                            android:visibility='@{cm0109m.checked? View.VISIBLE : View.GONE}'
                            app:maxValue="90"
                            app:minValue="1"
                            app:type="range" />

                        <RadioButton
                            android:id="@+id/cm010988"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/cm010988" />

                        <RadioButton
